{
	"name": "brotkrueml/typo3-matomo-integration",
	"type": "typo3-cms-extension",
	"description": "Matomo integration for TYPO3",
	"keywords": [
		"TYPO3 CMS",
		"TYPO3 extension",
		"Matomo"
	],
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "Chris MÃ¼ller",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=7.4",
		"ext-json": "*",
		"symfony/polyfill-php80": "^1.17",
		"typo3/cms-core": "^10.4.15 || ^11.3",
		"typo3/cms-frontend": "^10.4.15 || ^11.3"
	},
	"require-dev": {
		"ergebnis/composer-normalize": "^2.15",
		"friendsofphp/php-cs-fixer": "^2.19",
		"php-coveralls/php-coveralls": "^2.4",
		"phpunit/phpunit": "^9.5",
		"typo3/coding-standards": "^0.3"
	},
	"config": {
		"bin-dir": ".Build/bin",
		"sort-packages": true,
		"vendor-dir": ".Build/vendor"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		},
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"extension-key": "matomo_integration",
			"web-dir": ".Build/web"
		}
	},
	"autoload": {
		"psr-4": {
			"Brotkrueml\\MatomoIntegration\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Brotkrueml\\MatomoIntegration\\Tests\\": "Tests"
		}
	},
	"support": {
		"issues": "https://github.com/brotkrueml/typo3-matomo-integration/issues",
		"source": "https://github.com/brotkrueml/typo3-matomo-integration",
		"docs": "https://docs.typo3.org/p/brotkrueml/typo3-matomo-integration/master/en-us/"
	}
}
